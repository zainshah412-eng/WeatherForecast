<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SignatureView">
        <attr name="penMinWidth" format="dimension" />
        <attr name="penMaxWidth" format="dimension" />
        <attr name="penColor" format="color" />
        <attr name="velocityFilterWeight" format="float" />
        <attr name="clearOnDoubleClick" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTextView">
        <attr name="font_name">
            <enum value="1" name="objectiveBold"/>
            <enum value="2" name="objectiveExtraBold"/>
            <enum value="3" name="objectiveItalic"/>
            <enum value="4" name="objectiveLight"/>
            <enum value="5" name="objectiveMedium"/>
            <enum value="6" name="objectiveRegular"/>
        </attr>
    </declare-styleable>
</resources>